Options +FollowSymlinks
RewriteEngine on

RewriteCond %{HTTP_HOST} ^creham.com$
RewriteRule ^(.*) http://www.creham.com/$1 [QSA,L,R=301]

ErrorDocument 404 /index.php?erreur404
RewriteRule ^(.*)-p-(.*)$ index.php?ob=p&pg_id=$2&%{QUERY_STRING}
RewriteRule ^contact$ index.php?ob=c&%{QUERY_STRING}
RewriteRule ^espace$ index.php?ob=e&%{QUERY_STRING}
RewriteRule  ^([a-z0-9A-Z/-]*).htm     /index.php [PT]  [QSA]

